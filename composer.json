{
    "name": "studio24/laravel-accessible-forms",
    "description": "Create more accessible forms in your Laravel apps",
    "keywords": [
        "studio24",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/studio24/laravel-accessible-forms",
    "require": {
        "php": "^8.2",
        "studio24/accessible-forms": "dev-main"
    },
    "require-dev": {
        "illuminate/contracts": "^12.0",
        "orchestra/testbench": "^10.7",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": ">10",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-package-tools": "^1.92",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Studio24\\LaravelAccessibleForms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Studio24\\LaravelAccessibleForms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/phpcs",
        "format": "./vendor/bin/phpcbf",
        "phpstan": "./vendor/bin/phpstan"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Studio24\\AccessibleForms\\AccessibleFormsServiceProvider"
            ],
            "aliases": {
                "AccessibleForms": "Studio24\\AccessibleForms\\Facades\\AccessibleForms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
